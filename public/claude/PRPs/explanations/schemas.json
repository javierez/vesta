  "listings": {
    "listingType": {
      "type": "varchar(20)",
      "values": ["Sale", "Rent", "Transfer", "RentWithOption", "RoomSharing"],
      "normalize": {
        "Sale": ["Venta", "Compra"],
        "Rent": ["Alquiler", "Renta"],
        "Transfer": ["Traspaso"],
        "RentWithOption": ["Alquiler con opción"],
        "RoomSharing": ["Compartir piso", "Habitación"]
      },
      "spanish": ["tipo operación"],
      "notes": "⚠️ Required"
    },
    "status": {
      "type": "varchar(20)",
      "values": ["En Venta", "En Alquiler", "Vendido", "Alquilado", "Descartado", "Draft"],
      "rules": {
        "Sale": ["En Venta", "Vendido"],
        "Rent": ["En Alquiler", "Alquilado"]
      },
      "spanish": ["estado"],
      "notes": "⚠️ Must match listingType"
    },
    "price": {
      "type": "decimal(12,2)",
      "spanish": ["precio", "valor"],
      "notes": "⚠️ Required. For Sale=total, For Rent=monthly",
      "example": "150.000€ → 150000.00"
    },
    "description": {
      "type": "text",
      "spanish": ["descripción", "detalles"],
      "notes": "Detailed listing description (can be different from property description)"
