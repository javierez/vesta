{
  "properties": {
    "title": {
      "type": "varchar(255)",
      "spanish": ["título", "nombre"],
      "notes": "Auto-generated: 'Piso en Calle Mayor (Sol)'"
    },
    "description": {
      "type": "text",
      "spanish": ["descripción", "detalles"]
    },
    "propertyType": {
      "type": "varchar(20)",
      "values": ["piso", "casa", "local", "garaje", "solar"],
      "normalize": {
        "piso": ["apartamento", "estudio", "ático", "dúplex", "loft"],
        "casa": ["chalet", "adosada"],
        "local": ["oficina"],
        "garaje": ["parking"],
        "solar": ["terreno", "parcela"]
      },
      "notes": "⚠️ Must match exactly"
    },
    "propertySubtype": {
      "type": "varchar(50)",
      "rules": {
        "piso": ["Piso", "Apartment", "Ground floor", "Dúplex", "Ático", "Estudio", "Loft", "Bajo"],
        "casa": ["Casa", "Casa adosada", "Chalet", "Bungalow"],
        "local": ["Otros", "Oficinas", "Hotel"],
        "garaje": ["Individual", "Doble", "Moto"],
        "solar": ["Suelo residencial", "Suelo industrial"]
      },
      "notes": "⚠️ Must match propertyType"
    },
    "bedrooms": {
      "type": "smallint",
      "spanish": ["dormitorios", "habitaciones", "cuartos", "hab"],
      "range": "0-10"
    },
    "bathrooms": {
      "type": "decimal(3,1)",
      "spanish": ["baños", "aseos"],
      "example": "2.5"
    },
    "squareMeter": {
      "type": "int",
      "spanish": ["superficie", "metros", "m2", "m²"],
      "notes": "For garaje use builtSurfaceArea"
    },
    "builtSurfaceArea": {
      "type": "decimal(10,2)",
      "spanish": ["superficie construida", "metros construidos"]
    },
    "yearBuilt": {
      "type": "smallint",
      "spanish": ["año", "año construcción"],
      "range": "1800-2030"
    },
    "cadastralReference": {
      "type": "varchar(255)",
      "spanish": ["referencia catastral", "catastro"],
      "notes": "Can auto-populate many fields"
    },
    "conservationStatus": {
      "type": "smallint",
      "values": [1, 2, 3, 4, 6],
      "map": {
        "1": "Buen estado",
        "2": "A reformar",
        "3": "Casi nuevo",
        "4": "Para reformar",
        "6": "Reformado"
      },
      "notes": "⚠️ Return NUMBER not text! No code 5!"
    },
    "street": {
      "type": "varchar(255)",
      "spanish": ["dirección", "calle", "avenida"],
      "normalize": {
        "c/": "Calle",
        "av.": "Avenida",
        "pza.": "Plaza"
      }
    },
    "addressDetails": {
      "type": "varchar(255)",
      "spanish": ["piso", "puerta", "escalera"],
      "example": "3º B"
    },
    "postalCode": {
      "type": "varchar(20)",
      "spanish": ["código postal", "cp"],
      "pattern": "5 digits"
    },
    "neighborhoodId": {
      "type": "bigint",
      "notes": "From geocoding/cadastral service"
    },
    "latitude": {
      "type": "decimal(10,8)",
      "notes": "From geocoding service"
    },
    "longitude": {
      "type": "decimal(11,8)",
      "notes": "From geocoding service"
    },
    "energyCertificateStatus": {
      "type": "varchar(20)",
      "values": ["disponible", "en_tramite", "pendiente", "no_indicado", "exento"],
      "spanish": ["certificado energético"]
    },
    "energyConsumptionScale": {
      "type": "varchar(2)",
      "values": ["A", "B", "C", "D", "E", "F", "G"],
      "spanish": ["letra energética"],
      "notes": "⚠️ Single letter only!"
    },
    "energyConsumptionValue": {
      "type": "decimal(6,2)",
      "spanish": ["consumo energético", "kwh"],
      "unit": "kWh/m²·año"
    },
    "emissionsScale": {
      "type": "varchar(2)",
      "values": ["A", "B", "C", "D", "E", "F", "G"],
      "spanish": ["emisiones"],
      "notes": "⚠️ Single letter only!"
    },
    "emissionsValue": {
      "type": "decimal(6,2)",
      "spanish": ["valor emisiones"],
      "unit": "kg CO₂/m²·año"
    },
    "hasHeating": {
      "type": "boolean",
      "spanish": ["calefacción"],
      "logic": "explicit_only"
    },
    "heatingType": {
      "type": "varchar(50)",
